<h1><%= @state %>'s Representatives</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @representatives.each do |representative| %>
      <tr>
        <td><%= link_to 'Show', representative %></td>
        <td><%= representative.first_name %></td>
        <td><%= representative.last_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

