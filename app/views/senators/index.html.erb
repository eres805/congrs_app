<h1><%= @state %>'s Senators</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @senators.each do |senator| %>
      <tr>
        <td><%= link_to 'Show', senator %></td>
        <td><%= senator.first_name %></td>
        <td><%= senator.last_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

